<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OnlinemusicstoreClasses.AdminController">
    <children>
        <Label layoutX="193.0" layoutY="30.0" prefHeight="30.0" prefWidth="222.0" text="Change Account Settings" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
            <effect>
                <Glow level="1.0" />
            </effect></Label>
        <Button layoutX="551.0" layoutY="-1.0" mnemonicParsing="false" onAction="#logOut" text="Log out" />
      <TableView fx:id="accountTableView" layoutX="14.0" layoutY="91.0" prefHeight="203.0" prefWidth="578.0">
        <columns>
            <TableColumn fx:id="passwordColumn" prefWidth="292.0" text="Password" />
            <TableColumn fx:id="emailColumn" prefWidth="285.0" text="E-mail" />
        </columns>
      </TableView>
      <Button fx:id="changePassword" layoutX="86.0" layoutY="305.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="25.0" prefWidth="143.0" text="Change password" />
      <Button layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#changeToMainMenu" text="Back to Main Menu" />
      <Button fx:id="changeEmail" layoutX="387.0" layoutY="305.0" mnemonicParsing="false" onAction="#changeEmail" prefHeight="25.0" prefWidth="125.0" text="Change E-mail" />

    </children>
    <stylesheets>
        <URL value="@../OnlinemusicstoreClasses/style.css" />
    </stylesheets>
</AnchorPane>
