<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="OnlinemusicstoreClasses.PlaylistController">
   <children>
      <Label layoutX="247.0" layoutY="6.0" text="Bought Songs" />
      <Label layoutX="247.0" layoutY="163.0" text="Your Playlist" />
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#changetoMainmenu" text="Main Menu" />
      <Button layoutX="553.0" layoutY="325.0" mnemonicParsing="false" onAction="#playSong" text="Play" />
      <Button layoutX="474.0" layoutY="325.0" mnemonicParsing="false" onAction="#deletePlaylist" text="Remove" />
       <Button layoutX="503.0" layoutY="-1.0" mnemonicParsing="false" onAction="#helpMenuPressed" text="Help" />
      <TableView fx:id="StartTable" layoutY="23.0" prefHeight="140.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="Song" prefWidth="200" text="C1" />
          <TableColumn fx:id="Artist" prefWidth="200" text="C2" />
            <TableColumn fx:id="Album" prefWidth="200" text="C1" />
        </columns>
      </TableView>
      <TableView fx:id="tableAdded" layoutY="185.0" prefHeight="140.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="SongInPlaylist" prefWidth="200" text="C1" />
          <TableColumn fx:id="ArtistInPlaylist" prefWidth="200.0" text="C2" />
            <TableColumn fx:id="AlbumInPlaylist" prefWidth="200" text="C1" />
        </columns>
      </TableView>
      <Button layoutX="407.0" layoutY="325.0" mnemonicParsing="false" onAction="#clearPlaylist" text="Clear" />
   </children>
    <stylesheets>
        <URL value="@../OnlinemusicstoreClasses/style.css" />
    </stylesheets>
</AnchorPane>
