          <TableColumn fx:id="columnArtist" prefWidth="135.0" text="C2" />
            <TableColumn fx:id="columnAlbum" prefWidth="135.0" text="C2" />
            <TableColumn fx:id="columnPrice" prefWidth="135.0" text="C2" />
        </columns>
      </TableView>
      <Button fx:id="clearAll" layoutX="514.0" layoutY="339.0" mnemonicParsing="false" onAction="#deleteAll" text="Clear All" />
